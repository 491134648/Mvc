{
  "description": "Design time hosting infrastructure for the ASP.NET MVC Razor view engine.",
  "version": "6.0.0-rc1-final",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/mvc"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk"
  },
  "dependencies": {
    "Microsoft.AspNet.FileProviders.Physical": "1.0.0-rc1-final",
    "Microsoft.AspNet.Razor.Runtime": "4.0.0-rc1-final",
    "Microsoft.Extensions.Caching.Memory": "1.0.0-rc1-final",
    "Microsoft.Extensions.PropertyHelper.Sources": {
      "version": "1.0.0-rc1-final",
      "type": "build"
    }
  },
  "frameworks": {
    "net451": {
      "dependencies": {
        "Microsoft.Extensions.PlatformAbstractions": "1.0.0-rc1-final"
      }
    },
    "dotnet5.4": {
      "dependencies": {
        "Microsoft.Extensions.PlatformAbstractions": "1.0.0-rc1-final",
        "System.Collections.Concurrent": "4.0.11-beta-23516",
        "System.ComponentModel.TypeConverter": "4.0.1-beta-23516",
        "System.Reflection.Extensions": "4.0.1-beta-23516",
        "System.Text.Encoding.Extensions": "4.0.11-beta-23516"
      }
    }
  },
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}